{
  "name": "tidb-dashboard",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "lerna": "^3.22.1",
    "wsrun": "^5.2.1",
    "rimraf": "^3.0.2"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "plugins/*",
    "scripts"
  ],
  "scripts": {
    "clean": "wsrun -a rimraf build",
    "build:dev": "wsrun -x @tidb-dashboard/build-scripts -s -e -c webpack --config webpack.config.ts --progress --env development"
  }
}
